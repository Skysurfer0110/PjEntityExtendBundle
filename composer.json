{
  "name": "pjarmalavicius/entity-extend-bundle",
  "type": "symfony-bundle",
  "description": "Easy doctrine entities extending",
  "keywords": ["doctrine extend mapping"],
  "homepage": "https://github.com/pjarmalavicius/PjEntityExtendBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Paulius Jarmalavičius",
      "email": "paulius.jarmalavicius@gmail.com"
    }
  ],
  "repositories":
    [
            {
                "type": "vcs",
                "url": "https://github.com/Skysurfer0110/PjEntityExtendBundle"
            }
    ],
  "require": {
    "php": "~5.4|~7.0",
